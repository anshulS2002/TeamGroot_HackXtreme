{
  "manifest_version": 3,
  "name": "GameHub Extension",
  "version": "1.0",
  "description": "A Chrome extension for GameHub",
  "permissions": [
    "activeTab",
    "videoCapture",
    "https://www.whatsapp.com/*"
  ],
  "icons":{
    "16":"logo.png",
    "48":"logo.png"
  },
  "action": {
    "default_popup": "popup.html"
  }
}